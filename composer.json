{
    "name": "tbcd/rest-http-client",
    "description": "",
    "type": "library",
    "license": "MIT",
    "version": "6.0.0",
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "symfony/http-client": "^6.0",
        "symfony/http-foundation": "^6.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "TBCD\\RestHttpClient\\": "src/"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^6.0"
        }
    }
}
